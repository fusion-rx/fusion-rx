{
    "name": "@fusion-rx/postbuild",
    "version": "0.1.0",
    "description": "Executes post-build for Fusion ts compilations",
    "author": "Alexander Porrello",
    "license": "MIT",
    "main": "src/index.js",
    "type": "module",
    "bin": {
        "fsn": "cli.js"
    },
    "dependencies": {
        "chalk": "^5.3.0",
        "ora": "5.4.1"
    },
    "exports": {
        "./package.json": {
            "default": "./package.json"
        },
        ".": {
            "import": "./src/index.js",
            "types": "./src/types/index.d.ts",
            "default": "./src/index.js"
        }
    }
}
