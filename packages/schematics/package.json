{
    "name": "@fusion-rx/schematics",
    "version": "0.1.0",
    "description": "Defines Fusion schematics and their types.",
    "author": "Alexander Porrello",
    "license": "MIT",
    "source": "src/public-api.ts",
    "main": "../../dist/schematics/index.js",
    "module": "../../dist/schematics/index.mjs",
    "types": "../../dist/schematics/index.types.d.ts",
    "targets": {
        "main": {
            "context": "node",
            "outputFormat": "commonjs"
        },
        "module": {
            "context": "browser",
            "outputFormat": "esmodule"
        }
    },
    "scripts": {
        "postbuild": "node ../../tools/post-build/index.js",
        "build": "npx parcel build && npm run postbuild && cp -r src/**/*.json ../../dist/schematics"
    },
    "dependencies": {},
    "devDependencies": {},
    "exports": {
        "./package.json": {
            "default": "./package.json"
        },
        ".": {
            "node": "./index.js",
            "esm2020": "./index.mjs",
            "es2020": "./index.mjs",
            "types": "./index.types.d.ts",
            "default": "./index.js"
        }
    }
}
